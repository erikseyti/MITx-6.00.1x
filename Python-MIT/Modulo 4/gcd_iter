def gcdIter(a, b):
    '''
    a, b: positive integers
    
    returns: a positive integer, the greatest common divisor of a & b.
    '''
    # Your code here
    menorValor =0
    maiorDivisorComum =1
    if a> b:
        menorValor = b
    else:
        menorValor = a
    while menorValor == 1:
        if a / menorValor == int and b / menorValor == int:
            maiorDivisorComum = menorValor
        print(maiorDivisorComum)
    return maiorDivisorComum

x= gcdIter(2,12)
print(x)